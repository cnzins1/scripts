local function a()
    local r = game:GetService("ReplicatedStorage")
    local f = r:FindFirstChild("Auras")
    if f then
        for _, b in pairs(f:GetChildren()) do
            r.Remotes.AcceptAura:FireServer(b.Name, true)
        end
    end
end

local c = 0
while true do
    task.wait(0.1)
    game:GetService("ReplicatedStorage").Remotes.ZachRLL:InvokeServer()
    c = c + 1
    if c >= 5 then
        a()
        c = 0
    end
end

while true do
    wait(0.2)
    local a="1"
    local b=game:GetService("ReplicatedStorage").Remotes.DeleteAura
    b:FireServer("Heat",a)
    b:FireServer("Flames Curse",a)
    b:FireServer("Dark Matter",a)
    b:FireServer("Frigid",a)
    b:FireServer("Sorcerous",a)
    b:FireServer("Starstruck",a)
    b:FireServer("Voltage",a)
    b:FireServer("Constellar",a)
    b:FireServer("Iridescent",a)
    b:FireServer("Gale",a)
    b:FireServer("Shiver",a)
    b:FireServer("Bloom",a)
    b:FireServer("Fiend",a)
    b:FireServer("Tidal",a)
    b:FireServer("Flame",a)
    b:FireServer("Frost",a)
    b:FireServer("Antimatter",a)
    b:FireServer("Numerical",a)
    b:FireServer("Orbital",a)
    b:FireServer("Moonlit",a)
    b:FireServer("Glacial",a)
    b:FireServer("Bloom",a)
    b:FireServer("Prism",a)
    b:FireServer("Nebula",a)
    b:FireServer("Iridescent",a)
    b:FireServer("Cupid",a)
end
